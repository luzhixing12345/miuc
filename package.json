{
    "name": "miuc",
    "displayName": "miuc",
    "description": "markdown intelligent url complete",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.78.0"
    },
    "homepage": "https://github.com/luzhixing12345/miuc",
    "repository": {
        "type": "git",
        "url": "https://github.com/luzhixing12345/miuc.git"
    },
    "bugs": {
        "url": "https://github.com/luzhixing12345/miuc/issues"
    },
    "publisher": "kamilu",
    "icon": "./images/logo.png",
    "keywords": [
        "markdown",
        "url",
        "complete",
        "miuc"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "keybindings": [
            {
                "command": "miuc.myKeyBinding",
                "key": "ctrl+v",
                "mac": "cmd+v",
                "when": "editorLangId == markdown && editorTextFocus"
            },
            {
                "command": "miuc.tabKey",
                "key": "tab",
                "mac": "tab",
                "when": "editorLangId == markdown && editorTextFocus && editorHasSelection"
            }
        ],
        "languages": [
            {
                "id": "markdown",
                "extensions": [
                    ".md",
                    ".markdown",
                    ".mdown",
                    ".mkdn",
                    ".mkd",
                    ".mdwn",
                    ".mdtxt",
                    ".mdtext",
                    ".Rmd"
                ]
            }
        ]
    },
    "extensionDependencies": [
        "ms-python.python"
    ],
    "python": {
        "extensionDependencies": [
            "miuc"
        ]
    },
    "scripts": {
        "vscode:prepublish": "pnpm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "pnpm run compile && pnpm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.78.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "iconv-lite": "^0.6.3"
    }
}